{
  "name": "jscoinWallet",
  "version": "0.0.1",
  "description": "jscoin Wallet",
  "scripts": {
    "lint": "eslint --ext .js --ext .jsx .",
    "lint:fix": "eslint --ext .js --ext .jsx . --fix ",
    "build": "npm run build:clean && webpack --config config/webpack.config.js --color -p --progress",
    "start": "node server",
    "postinstall": "if [ $BUILD ]; then npm run build; fi",
    "build:clean": "rimraf ./public",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test jest",
    "forever": "forever server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dpanchenko/jscoinWallet.git"
  },
  "keywords": [],
  "author": "Dmitry Panchenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dpanchenko/jscoinWallet/issues"
  },
  "homepage": "https://github.com/dpanchenko/jscoinWallet#readme",
  "precommit": [
    "lint"
  ],
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/index.jsx",
      "!app/routes.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/config/bundler.js",
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-react": "^7.9.1",
    "react-test-renderer": "^16.4.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.11.0",
    "jest": "^23.1.0",
    "jquery": "^3.3.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "localStorage": "^1.0.3",
    "moment": "^2.22.2",
    "node-sass": "^4.9.0",
    "npm": "^6.1.0",
    "popper.js": "^1.14.3",
    "query-string": "^6.1.0",
    "raf": "^3.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "whatwg-fetch": "^2.0.4"
  },
  "main": "server.js"
}
