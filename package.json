{
  "name": "jscoinWallet",
  "version": "0.0.1",
  "description": "jscoin Wallet",
  "scripts": {
    "lint": "eslint --ext .js --ext .jsx .",
    "lint:fix": "eslint --ext .js --ext .jsx . --fix ",
    "build": "npm run build:clean && webpack --config config/webpack.config.js --color -p --progress",
    "start": "node server",
    "postinstall": "if [ $BUILD ]; then npm run build; fi",
    "build:clean": "rimraf ./public",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dpanchenko/jscoinWallet.git"
  },
  "keywords": [],
  "author": "Dmitry Panchenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dpanchenko/jscoinWallet/issues"
  },
  "homepage": "https://github.com/dpanchenko/jscoinWallet#readme",
  "precommit": [
    "lint"
  ],
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/index.jsx",
      "!app/routes.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/config/bundler.js",
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "react-test-renderer": "^16.0.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-hot-middleware": "^2.19.1"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.1",
    "html-webpack-plugin": "^2.30.1",
    "install": "^0.10.1",
    "jest": "^21.2.1",
    "jquery": "^3.3.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "localStorage": "^1.0.3",
    "moment": "^2.20.1",
    "node-sass": "^4.5.3",
    "query-string": "^5.1.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "main": "server.js"
}
